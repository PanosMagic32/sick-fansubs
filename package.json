{
  "name": "sick",
  "version": "0.2.2",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx serve",
    "start:fe": "nx serve sick-fansubs",
    "start:be": "nx serve api",
    "dev": "nx run-many --target=serve --projects=api,sick-fansubs --parallel",
    "build": "nx build",
    "build:all": "nx run-many --target=build --projects=api,sick-fansubs --parallel --prod --skip-nx-cache",
    "test": "nx test",
    "deploy:docker": "npm run build:all && cp .env dist/apps/api/ && cp .env dist/apps/sick-fansubs/ && cp .config.json dist/apps/sick-fansubs/ && docker compose up",
    "prepack": "rm -rf dist/ && npm run build:all",
    "pack": "docker compose up -d --always-recreate-deps --force-recreate",
    "prepare": "husky install",
    "format:check": "prettier apps/**/*.{ts,html,scss,json} libs/**/*.{ts,html,scss,json} --check",
    "format:fix": "prettier apps/**/*.{ts,html,scss,json} libs/**/*.{ts,html,scss,json} --write",
    "pretty-quick": "pretty-quick --pattern \"**/*.*(js|ts|css|scss|html)\" --staged"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.12",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.2.12",
    "@angular/compiler": "^14.2.12",
    "@angular/core": "^14.2.12",
    "@angular/forms": "^14.2.12",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.2.12",
    "@angular/platform-browser-dynamic": "^14.2.12",
    "@angular/router": "^14.2.12",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/swagger": "^6.1.3",
    "@nrwl/angular": "15.1.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "mongoose": "^6.7.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.7",
    "swagger-ui-express": "^4.6.0",
    "tslib": "^2.4.1",
    "zone.js": "^0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular-eslint/eslint-plugin": "^14.4.0",
    "@angular-eslint/eslint-plugin-template": "^14.4.0",
    "@angular-eslint/template-parser": "^14.4.0",
    "@angular/cli": "^14.2.10",
    "@angular/compiler-cli": "^14.2.12",
    "@angular/language-service": "^14.2.12",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.1",
    "@nrwl/cli": "15.1.0",
    "@nrwl/cypress": "15.1.0",
    "@nrwl/eslint-plugin-nx": "15.1.0",
    "@nrwl/jest": "15.1.0",
    "@nrwl/linter": "15.1.0",
    "@nrwl/nest": "15.1.0",
    "@nrwl/node": "15.1.0",
    "@nrwl/workspace": "15.1.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^28.1.8",
    "@types/node": "^18.11.9",
    "@types/passport-jwt": "^3.0.7",
    "@types/passport-local": "^1.0.34",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "cypress": "^11.2.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "husky": "^8.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-preset-angular": "^12.2.3",
    "lint-staged": "^13.0.4",
    "nx": "15.1.0",
    "prettier": "^2.8.0",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.8.4",
    "validate-branch-name": "^1.3.0"
  },
  "husky": {
    "hooks": "npm run format:fix && npx validate-branch-name && npx lint-staged"
  },
  "validate-branch-name": {
    "pattern": "^(main|develop){1}$|^(feature|bugfix|hotfix|release)-.+$/g",
    "errorMsg": "Branch name has to be main or develop or has to start with feature-, bugfix-, hotfix- or release-"
  },
  "lint-staged": {
    "**/*.{ts,html,scss,json}": "prettier --write --ignore-unknown"
  }
}
