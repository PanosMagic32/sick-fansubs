{
  "name": "sick",
  "version": "0.5.4",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js",
    "start": "nx serve",
    "start:fe": "nx serve sick-fansubs",
    "start:be": "nx serve api",
    "dev": "nx run-many --target=serve --projects=api,sick-fansubs --parallel",
    "build": "nx build",
    "build:all": "nx run-many --target=build --projects=api,sick-fansubs --parallel --prod --skip-nx-cache",
    "test": "nx test",
    "lint-staged": "lint-staged --allow-empty",
    "deploy:docker": "npm run build:all && cp .env dist/apps/api/ && cp .env dist/apps/sick-fansubs/ && cp .config.json dist/apps/sick-fansubs/ && docker compose up",
    "prepack": "rm -rf dist/ && npm run build:all",
    "pack": "docker compose up -d --always-recreate-deps --force-recreate",
    "prepare": "husky install",
    "format:check": "prettier apps/**/*.{ts,html,scss,json} libs/**/*.{ts,html,scss,json} --check",
    "format:fix": "prettier apps/**/*.{ts,html,scss,json} libs/**/*.{ts,html,scss,json} --write",
    "pretty-quick": "pretty-quick --pattern \"**/*.*(js|ts|css|scss|html)\" --staged"
  },
  "husky": {
    "hooks": "npm run format:fix && npx validate-branch-name && npx lint-staged"
  },
  "validate-branch-name": {
    "pattern": "^(main|develop){1}$|^(feature|bugfix|hotfix|release)-.+$/g",
    "errorMsg": "Branch name has to be main or develop or has to start with feature-, bugfix-, hotfix- or release-"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        1,
        "always",
        100
      ]
    }
  },
  "lint-staged": {
    "*.{ts,js,html,css,scss,less,md,json}": "prettier --write --ignore-unknown"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.0.2",
    "@angular/cdk": "17.0.0",
    "@angular/common": "17.0.2",
    "@angular/compiler": "17.0.2",
    "@angular/core": "17.0.2",
    "@angular/forms": "17.0.2",
    "@angular/material": "17.0.0",
    "@angular/platform-browser": "17.0.2",
    "@angular/platform-browser-dynamic": "17.0.2",
    "@angular/router": "17.0.2",
    "@nestjs/common": "^10.2.8",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.8",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mongoose": "^10.0.2",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.8",
    "@nestjs/swagger": "^7.1.15",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "mongoose": "^7.6.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "tslib": "^2.6.2",
    "zone.js": "0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.0.0",
    "@angular-devkit/core": "17.0.0",
    "@angular-devkit/schematics": "17.0.0",
    "@angular-eslint/eslint-plugin": "17.0.1",
    "@angular-eslint/eslint-plugin-template": "17.0.1",
    "@angular-eslint/template-parser": "17.0.1",
    "@angular/cli": "~17.0.0",
    "@angular/compiler-cli": "17.0.2",
    "@angular/language-service": "17.0.2",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.8",
    "@nx/angular": "17.1.1",
    "@nx/cypress": "17.1.1",
    "@nx/eslint": "17.1.1",
    "@nx/eslint-plugin": "17.1.1",
    "@nx/jest": "17.1.1",
    "@nx/nest": "17.1.1",
    "@nx/node": "17.1.1",
    "@nx/workspace": "17.1.1",
    "@schematics/angular": "17.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "cypress": "^13.5.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "13.1.4",
    "lint-staged": "^13.3.0",
    "nx": "17.1.1",
    "prettier": "^3.0.3",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "5.2.2",
    "validate-branch-name": "^1.3.0"
  }
}
