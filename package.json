{
  "name": "sick",
  "version": "0.17.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "start": "nx serve",
    "start:fe": "nx serve web",
    "start:be": "nx serve api",
    "dev": "nx run-many --target=serve --projects=api,web --parallel",
    "build": "nx build",
    "build:all": "nx run-many --target=build --projects=api,web --parallel --prod --skip-nx-cache",
    "test": "nx test",
    "lint-staged": "lint-staged --allow-empty",
    "deploy:docker": "pnpm run build:all && cp .env dist/apps/api/ && cp .env dist/apps/web/ && cp .config.json dist/apps/web/ && docker compose up",
    "prepack": "rm -rf dist/ && pnpm run build:all",
    "pack": "docker compose up -d --always-recreate-deps --force-recreate",
    "prepare": "husky install",
    "format:check": "prettier apps/**/*.{ts,html,scss,json} libs/**/*.{ts,html,scss,json} --check",
    "format:fix": "prettier apps/**/*.{ts,html,scss,json} libs/**/*.{ts,html,scss,json} --write",
    "pretty-quick": "pretty-quick --pattern \"**/*.*(js|ts|css|scss|html)\" --staged"
  },
  "husky": {
    "hooks": "pnpm run format:fix && npx validate-branch-name && npx lint-staged"
  },
  "validate-branch-name": {
    "pattern": "^(main|develop){1}$|^(feature|bugfix|hotfix|release)-.+$/g",
    "errorMsg": "Branch name has to be main or develop or has to start with feature-, bugfix-, hotfix- or release-"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        1,
        "always",
        100
      ]
    }
  },
  "lint-staged": {
    "*.{ts,js,html,css,scss,less,md,json}": "prettier --write --ignore-unknown"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~21.0.6",
    "@angular/cdk": "~21.0.5",
    "@angular/common": "~21.0.6",
    "@angular/compiler": "~21.0.6",
    "@angular/core": "~21.0.6",
    "@angular/forms": "~21.0.6",
    "@angular/material": "~21.0.5",
    "@angular/platform-browser": "~21.0.6",
    "@angular/platform-browser-dynamic": "~21.0.6",
    "@angular/router": "~21.0.6",
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/mongoose": "^11.0.4",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/swagger": "^11.2.3",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "mongoose": "^8.21.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~21.0.4",
    "@angular-devkit/core": "~21.0.4",
    "@angular-devkit/schematics": "~21.0.4",
    "@angular-eslint/eslint-plugin": "21.1.0",
    "@angular-eslint/eslint-plugin-template": "21.1.0",
    "@angular-eslint/template-parser": "21.1.0",
    "@angular/cli": "~21.0.4",
    "@angular/compiler-cli": "~21.0.6",
    "@angular/language-service": "~21.0.6",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.11",
    "@nx/angular": "22.3.3",
    "@nx/eslint": "22.3.3",
    "@nx/eslint-plugin": "22.3.3",
    "@nx/js": "22.3.3",
    "@nx/nest": "22.3.3",
    "@nx/node": "22.3.3",
    "@nx/webpack": "22.3.3",
    "@nx/workspace": "22.3.3",
    "@schematics/angular": "~21.0.4",
    "@swc-node/register": "^1.11.1",
    "@swc/core": "^1.15.8",
    "@swc/helpers": "^0.5.18",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^25.0.3",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@typescript-eslint/utils": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^13.3.0",
    "nx": "22.3.3",
    "prettier": "^3.7.4",
    "pretty-quick": "^4.2.2",
    "ts-node": "^10.9.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~5.9.3",
    "validate-branch-name": "^1.3.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@parcel/watcher",
      "@scarf/scarf",
      "@swc/core",
      "bcrypt",
      "esbuild",
      "less",
      "lmdb",
      "msgpackr-extract",
      "nx",
      "oxc-resolver",
      "unrs-resolver"
    ]
  }
}
