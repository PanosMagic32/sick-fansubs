{
  "name": "sick",
  "version": "0.5.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js",
    "start": "nx serve",
    "start:fe": "nx serve sick-fansubs",
    "start:be": "nx serve api",
    "dev": "nx run-many --target=serve --projects=api,sick-fansubs --parallel",
    "build": "nx build",
    "build:all": "nx run-many --target=build --projects=api,sick-fansubs --parallel --prod --skip-nx-cache",
    "test": "nx test",
    "lint-staged": "lint-staged --allow-empty",
    "deploy:docker": "npm run build:all && cp .env dist/apps/api/ && cp .env dist/apps/sick-fansubs/ && cp .config.json dist/apps/sick-fansubs/ && docker compose up",
    "prepack": "rm -rf dist/ && npm run build:all",
    "pack": "docker compose up -d --always-recreate-deps --force-recreate",
    "prepare": "husky install",
    "format:check": "prettier apps/**/*.{ts,html,scss,json} libs/**/*.{ts,html,scss,json} --check",
    "format:fix": "prettier apps/**/*.{ts,html,scss,json} libs/**/*.{ts,html,scss,json} --write",
    "pretty-quick": "pretty-quick --pattern \"**/*.*(js|ts|css|scss|html)\" --staged"
  },
  "husky": {
    "hooks": "npm run format:fix && npx validate-branch-name && npx lint-staged"
  },
  "validate-branch-name": {
    "pattern": "^(main|develop){1}$|^(feature|bugfix|hotfix|release)-.+$/g",
    "errorMsg": "Branch name has to be main or develop or has to start with feature-, bugfix-, hotfix- or release-"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        1,
        "always",
        100
      ]
    }
  },
  "lint-staged": {
    "*.{ts,js,html,css,scss,less,md,json}": "prettier --write --ignore-unknown"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.0.0",
    "@angular/cdk": "16.0.0",
    "@angular/common": "16.0.0",
    "@angular/compiler": "16.0.0",
    "@angular/core": "16.0.0",
    "@angular/forms": "16.0.0",
    "@angular/material": "16.0.0",
    "@angular/platform-browser": "16.0.0",
    "@angular/platform-browser-dynamic": "16.0.0",
    "@angular/router": "16.0.0",
    "@nestjs/common": "^9.4.1",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.4.1",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/mongoose": "^9.2.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.1",
    "@nestjs/swagger": "6.3.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "mongoose": "^6.11.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^4.6.3",
    "tslib": "^2.5.2",
    "zone.js": "^0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.0.0",
    "@angular-devkit/core": "16.0.0",
    "@angular-devkit/schematics": "16.0.0",
    "@angular-eslint/eslint-plugin": "16.0.1",
    "@angular-eslint/eslint-plugin-template": "16.0.1",
    "@angular-eslint/template-parser": "16.0.1",
    "@angular/cli": "~16.0.0",
    "@angular/compiler-cli": "16.0.0",
    "@angular/language-service": "16.0.0",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.1",
    "@nx/angular": "16.2.1",
    "@nx/cypress": "16.2.1",
    "@nx/eslint-plugin": "16.2.1",
    "@nx/jest": "16.2.1",
    "@nx/linter": "16.2.1",
    "@nx/nest": "16.2.1",
    "@nx/node": "16.2.1",
    "@nx/workspace": "16.2.1",
    "@schematics/angular": "16.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.14",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "cypress": "^12.12.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-preset-angular": "^13.1.1",
    "lint-staged": "^13.2.2",
    "nx": "16.2.1",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "5.0.4",
    "validate-branch-name": "^1.3.0"
  }
}
