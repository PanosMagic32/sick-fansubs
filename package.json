{
  "name": "sick",
  "version": "0.4.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx serve",
    "start:fe": "nx serve sick-fansubs",
    "start:be": "nx serve api",
    "dev": "nx run-many --target=serve --projects=api,sick-fansubs --parallel",
    "build": "nx build",
    "build:all": "nx run-many --target=build --projects=api,sick-fansubs --parallel --prod --skip-nx-cache",
    "test": "nx test",
    "lint-staged": "lint-staged --allow-empty",
    "deploy:docker": "npm run build:all && cp .env dist/apps/api/ && cp .env dist/apps/sick-fansubs/ && cp .config.json dist/apps/sick-fansubs/ && docker compose up",
    "prepack": "rm -rf dist/ && npm run build:all",
    "pack": "docker compose up -d --always-recreate-deps --force-recreate",
    "prepare": "husky install",
    "format:check": "prettier apps/**/*.{ts,html,scss,json} libs/**/*.{ts,html,scss,json} --check",
    "format:fix": "prettier apps/**/*.{ts,html,scss,json} libs/**/*.{ts,html,scss,json} --write",
    "pretty-quick": "pretty-quick --pattern \"**/*.*(js|ts|css|scss|html)\" --staged"
  },
  "husky": {
    "hooks": "npm run format:fix && npx validate-branch-name && npx lint-staged"
  },
  "validate-branch-name": {
    "pattern": "^(main|develop){1}$|^(feature|bugfix|hotfix|release)-.+$/g",
    "errorMsg": "Branch name has to be main or develop or has to start with feature-, bugfix-, hotfix- or release-"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        1,
        "always",
        100
      ]
    }
  },
  "lint-staged": {
    "*.{ts,js,html,css,scss,less,md,json}": "prettier --write --ignore-unknown"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.1.0",
    "@angular/cdk": "15.1.0",
    "@angular/common": "15.1.0",
    "@angular/compiler": "15.1.0",
    "@angular/core": "15.1.0",
    "@angular/forms": "15.1.0",
    "@angular/material": "15.1.0",
    "@angular/platform-browser": "15.1.0",
    "@angular/platform-browser-dynamic": "15.1.0",
    "@angular/router": "15.1.0",
    "@nestjs/common": "^9.3.7",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.7",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.7",
    "@nestjs/swagger": "^6.2.1",
    "@nrwl/angular": "15.6.3",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "mongoose": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "swagger-ui-express": "^4.6.0",
    "tslib": "^2.5.0",
    "zone.js": "^0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.1.1",
    "@angular-eslint/eslint-plugin": "^15.2.0",
    "@angular-eslint/eslint-plugin-template": "^15.2.0",
    "@angular-eslint/template-parser": "^15.2.0",
    "@angular/cli": "~15.1.0",
    "@angular/compiler-cli": "15.1.0",
    "@angular/language-service": "15.1.0",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.7",
    "@nrwl/cli": "15.6.3",
    "@nrwl/cypress": "15.6.3",
    "@nrwl/eslint-plugin-nx": "15.6.3",
    "@nrwl/jest": "15.6.3",
    "@nrwl/linter": "15.6.3",
    "@nrwl/nest": "15.6.3",
    "@nrwl/node": "15.6.3",
    "@nrwl/workspace": "15.6.3",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^28.1.8",
    "@types/node": "^18.13.0",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "cypress": "^12.5.1",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-cypress": "^2.12.1",
    "husky": "^8.0.3",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-preset-angular": "^12.2.6",
    "lint-staged": "^13.1.1",
    "nx": "15.6.3",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.8.4",
    "validate-branch-name": "^1.3.0"
  }
}
